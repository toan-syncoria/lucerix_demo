<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Work Orders Planning -->
        <record id="mrp_production_workorder_tree_editable_view_inherit_planning" model="ir.ui.view">
            <field name="name">mrp.production.work.order.tree.editable.inherit.planning</field>
            <field name="model">mrp.workorder</field>
            <field name="inherit_id" ref="mrp.mrp_production_workorder_tree_editable_view"/>
            <field name="arch" type="xml">
            <!-- time_ids (mrp.workcenter.productivity) -->
                <xpath expr="//field[@name='state']" position="after">
                    <field name="person_mins_wkly" />
                    <!-- <field name="week_number" /> -->
                </xpath>
            </field>
        </record>
    </data>
</odoo>
